generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
}

model User {
  id Int @default(autoincrement()) @id
  name String
  email String @unique
  password String
  active Boolean @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime? @updatedAt
}
